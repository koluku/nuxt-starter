{
  "name": "nuxt-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "node lib/convertWebp.js & nuxt-ts generate",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .eslintignore . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .eslintignore --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "webp": "node lib/convertWebp.js",
    "server": "http-server public -g -b"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.4",
    "core-js": "^3.8.1",
    "nuxt": "^2.14.12",
    "nuxt-compress": "^4.1.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.3.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.3.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.6",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "lint-staged": "^10.5.3",
    "lqip-loader": "^2.2.1",
    "modern-css-reset": "^1.3.0",
    "node-sass": "^5.0.0",
    "nuxt-lazysizes": "^1.1.2",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "responsive-loader": "^2.2.1",
    "sass-loader": "^10.0.4",
    "sharp": "^0.26.2",
    "sqip-loader": "^1.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.18.0",
    "webp-loader": "^0.6.0"
  },
  "license": "UNLICENSED",
  "browserslist": [
    "defaults",
    "android >= 4.4",
    "IE 11"
  ]
}
